@model IEnumerable<ProdajnikWebController.Models.ObjectWork>

@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Object)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Object.ObjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkType.WorkTypeId)
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
